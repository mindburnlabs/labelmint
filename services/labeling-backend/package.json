{
  "name": "@labelmint/labeling-backend",
  "version": "1.0.0",
  "description": "LabelMint - Primary labeling service with TON/USDT micropayments",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "clean": "rm -rf dist",
    "test": "node test/endpoints.test.js",
    "test:enhanced": "node test/enhanced-features-test.js",
    "test:ai": "node test/ai-features-test.js",
    "test:growth": "node test/growth-automation-test.js",
    "test:viral": "node test/viral-features-test.js",
    "test:all": "npm run test && npm run test:enhanced && npm run test:ai && npm run test:growth && npm run test:viral",
    "test:unit": "jest",
    "test:unit:watch": "jest --watch",
    "test:unit:coverage": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:visual": "percy exec -- playwright test",
    "test:load": "echo 'Load testing with k6 coming soon'",
    "test:load:stress": "echo 'Stress testing with k6 coming soon'"
  },
  "dependencies": {
    "@labelmint/shared": "workspace:*",
    "@aws-sdk/client-s3": "^3.600.0",
    "@aws-sdk/s3-request-presigner": "^3.600.0",
    "express": "^4.21.2",
    "cors": "^2.8.5",
    "helmet": "^8.0.0",
    "dotenv": "^16.4.7",
    "pg": "^8.16.3",
    "redis": "^4.7.0",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.9.16",
    "node-cron": "^3.0.3",
    "sharp": "^0.34.4",
    "file-type": "^19.0.0",
    "multer": "^1.4.5-lts.1",
    "uuid": "^10.0.0",
    "mime-types": "^2.1.35",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.10.0",
    "@types/pg": "^8.11.10",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/nodemailer": "^6.4.14",
    "@types/node-cron": "^3.0.11",
    "@types/multer": "^1.4.12",
    "@types/mime-types": "^2.1.4",
    "@types/jest": "^29.5.8",
    "@types/supertest": "^2.0.16",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "@playwright/test": "^1.44.0",
    "percy": "^0.28.6",
    "cross-env": "^7.0.3"
  }
}
