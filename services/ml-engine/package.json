{
  "name": "@labelmint/ml-engine",
  "version": "1.0.0",
  "description": "Machine Learning Engine for fraud detection and predictive analytics",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && tsc-alias",
    "dev": "tsx watch src/index.ts",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "type-check": "tsc --noEmit",
    "train:fraud": "tsx src/scripts/train-fraud-models.ts",
    "train:behavior": "tsx src/scripts/train-behavior-models.ts",
    "train:prediction": "tsx src/scripts/train-prediction-models.ts",
    "deploy:models": "tsx src/scripts/deploy-models.ts",
    "monitor:models": "tsx src/scripts/model-monitor.ts",
    "evaluate:models": "tsx src/scripts/evaluate-models.ts"
  },
  "dependencies": {
    "@tensorflow/tfjs": "^4.22.0",
    "@tensorflow/tfjs-node": "^4.22.0",
    "@tensorflow/tfjs-node-gpu": "^4.22.0",
    "@prisma/client": "^6.2.0",
    "redis": "^5.9.0",
    "ioredis": "^5.8.2",
    "postgres": "^3.4.7",
    "zod": "^3.25.0",
    "winston": "^3.18.3",
    "node-cron": "^4.2.1",
    "express": "^4.21.0",
    "cors": "^2.8.5",
    "helmet": "^8.1.0",
    "compression": "^1.8.1",
    "express-rate-limit": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^3.0.2",
    "uuid": "^11.0.0",
    "axios": "^1.12.2",
    "ml-matrix": "^6.12.0",
    "ml-regression": "^6.0.1",
    "simple-statistics": "^7.8.0",
    "moment": "^2.30.1",
    "lodash": "^4.17.21",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0",
    "sharp": "^0.34.4",
    "node-cache": "^5.1.2",
    "bull": "^4.16.0",
    "amqplib": "^0.10.4",
    "prom-client": "^15.1.0"
  },
  "devDependencies": {
    "@types/node": "^24.9.1",
    "@types/express": "^5.0.4",
    "@types/cors": "^2.8.19",
    "@types/compression": "^1.8.1",
    "@types/bcryptjs": "^3.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/uuid": "^11.0.0",
    "@types/lodash": "^4.17.7",
    "@types/node-cron": "^3.0.11",
    "@types/multer": "^1.4.12",
    "@types/node-cache": "^4.2.5",
    "@types/bull": "^4.10.4",
    "@types/amqplib": "^0.10.5",
    "typescript": "^5.9.3",
    "tsx": "^4.20.6",
    "tsc-alias": "^1.8.10",
    "vitest": "^4.0.3",
    "@vitest/coverage-v8": "^4.0.3",
    "eslint": "^9.38.0",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2"
  },
    "keywords": [
    "machine-learning",
    "fraud-detection",
    "anomaly-detection",
    "predictive-analytics",
    "tensorflow",
    "nodejs"
  ],
  "author": "LabelMint ML Team",
  "license": "MIT"
}