# Bot Tokens (get from @BotFather on Telegram)
CLIENT_BOT_TOKEN=your_client_bot_token_here
WORKER_BOT_TOKEN=your_worker_bot_token_here

# Environment
NODE_ENV=development
LOG_LEVEL=debug

# Redis Configuration
REDIS_URL=redis://localhost:6379
REDIS_PASSWORD=

# API URLs
LABELING_API_URL=http://localhost:3001
PAYMENT_API_URL=http://localhost:3000
WS_URL=ws://localhost:3001

# Database (if needed directly)
DATABASE_URL=postgresql://user:password@localhost:5432/labelmint

# External Services
OPENAI_API_KEY=your_openai_key_here
TELEGRAM_API_ID=your_api_id
TELEGRAM_API_HASH=your_api_hash

# Security
JWT_SECRET=your_jwt_secret_here
SESSION_SECRET=your_session_secret_here

# Features
ENABLE_ANALYTICS=true
ENABLE_NOTIFICATIONS=true
ENABLE_TRAINING=true
MIN_WITHDRAWAL_AMOUNT=5.00

# Rate Limiting
RATE_LIMIT_REQUESTS=30
RATE_LIMIT_WINDOW=60000

# Webhook Configuration
WEBHOOK_DOMAIN=your-domain.com
WEBHOOK_PATH=/api/bot/webhook
WEBHOOK_SECRET=your_webhook_secret