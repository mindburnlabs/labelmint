{
  "securityHeaders": {
    "helmet": {
      "contentSecurityPolicy": {
        "directives": {
          "defaultSrc": [
            "'self'"
          ],
          "styleSrc": [
            "'self'",
            "'unsafe-inline'"
          ],
          "scriptSrc": [
            "'self'"
          ],
          "imgSrc": [
            "'self'",
            "data:",
            "https:"
          ],
          "connectSrc": [
            "'self'",
            "https://api.labelmint.it"
          ],
          "fontSrc": [
            "'self'"
          ],
          "objectSrc": [
            "'none'"
          ],
          "mediaSrc": [
            "'self'"
          ],
          "frameSrc": [
            "'none'"
          ]
        }
      },
      "hsts": {
        "maxAge": 31536000,
        "includeSubDomains": true,
        "preload": true
      }
    }
  },
  "inputValidation": {
    "maxRequestSize": "10mb",
    "rateLimitWindow": 900000,
    "rateLimitMax": 100,
    "allowedOrigins": [
      "https://labelmint.it",
      "https://staging.labelmint.it",
      "https://admin.labelmint.it"
    ]
  },
  "monitoring": {
    "suspiciousPatterns": [
      {},
      {},
      {},
      {},
      {}
    ]
  }
}