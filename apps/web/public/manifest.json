{
  "name": "LabelMint - Data Labeling Platform",
  "short_name": "LabelMint",
  "description": "Professional data labeling marketplace with TON/USDT micropayments",
  "start_url": "/",
  "display": "standalone",
  "background_color": "#ffffff",
  "theme_color": "#4F46E5",
  "orientation": "portrait-primary",
  "scope": "/",
  "lang": "en",
  "categories": ["business", "productivity"],
  "icons": [
