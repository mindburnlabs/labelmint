    },
    {
      "src": "/icons/icon-512x512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "maskable any"
    }
  ],
  "screenshots": [
    {
      "src": "/screenshots/mobile-1.png",
      "sizes": "1280x720",
      "type": "image/png",
      "form_factor": "narrow"
    },
    {
      "src": "/screenshots/desktop-1.png",
      "sizes": "1280x720",
      "type": "image/png",
      "form_factor": "wide"
    }
  ],
  "shortcuts": [
    {
      "name": "New Task",
      "short_name": "New Task",
      "description": "Create a new labeling task",
      "url": "/dashboard/new-task",
      "icons": [{ "src": "/icons/new-task.png", "sizes": "96x96" }]
    },
    {
      "name": "My Projects",
      "short_name": "Projects",
      "description": "View my projects",
      "url": "/dashboard/projects",
      "icons": [{ "src": "/icons/projects.png", "sizes": "96x96" }]
    },
    {
      "name": "Earnings",
      "short_name": "Earnings",
      "description": "View my earnings",
      "url": "/dashboard/earnings",
      "icons": [{ "src": "/icons/earnings.png", "sizes": "96x96" }]
    }
  ],
  "share_target": {
    "action": "/share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "files": [
        {
          "name": "files",
          "accept": ["image/*", "audio/*", "text/*", ".csv", ".json"]
        }
      ]
    }
  },
  "protocol_handlers": [
    {
      "protocol": "web+labelmint",
      "url": "/handle-protocol?type=%s"
    }
  ],
  "related_applications": [
    {
      "platform": "play_store",
      "url": "https://play.google.com/store/apps/details?id=com.labelmint.app",
      "id": "com.labelmint.app"
    }
  ],
  "prefer_related_applications": false,
  "edge_side_panel": {
    "preferred_width": 400
  },
  "launch_handler": {
    "client_mode": "focus-existing"
  }
}