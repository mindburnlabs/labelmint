# LabelMint Web Application Environment Variables

# Application Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXT_PUBLIC_APP_NAME=LabelMint
NEXT_PUBLIC_APP_DESCRIPTION=Telegram Data Labeling Platform with TON/USDT Micropayments

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=https://pznxwcgduzkmksmuapgu.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# API Configuration
NEXT_PUBLIC_API_URL=http://localhost:3001/api
NEXT_PUBLIC_WS_URL=ws://localhost:3001
API_BASE_URL=http://localhost:3001

# Authentication
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your_nextauth_secret_here

# Telegram Configuration
NEXT_PUBLIC_TELEGRAM_BOT_USERNAME=labelmint_bot
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here

# TON/Blockchain Configuration
TON_NETWORK=testnet
USDT_CONTRACT_ADDRESS=testnet_address_here
TONCENTER_API_KEY=your_toncenter_api_key_here

# Feature Flags
NEXT_PUBLIC_ENABLE_ANALYTICS=false
NEXT_PUBLIC_ENABLE_PWA=true
NEXT_PUBLIC_ENABLE_DARK_MODE=true
NEXT_PUBLIC_ENABLE_TELEGRAM_WEB_APP=true

# Development
NODE_ENV=development
NEXT_PUBLIC_DEV_MODE=true

# Monitoring and Error Tracking
NEXT_PUBLIC_SENTRY_DSN=your_sentry_dsn_here
NEXT_PUBLIC_GOOGLE_ANALYTICS_ID=your_ga_id_here

# Redis Configuration (for sessions/caching)
REDIS_URL=redis://localhost:6379
REDIS_PASSWORD=your_redis_password_here

# Email Configuration (for notifications)
RESEND_API_KEY=your_resend_api_key_here
FROM_EMAIL=noreply@labelmint.com

# File Storage
NEXT_PUBLIC_S3_BUCKET=labelmint-assets
AWS_ACCESS_KEY_ID=your_aws_access_key_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_key_here
AWS_REGION=us-east-1

# AI/ML Services
NEXT_PUBLIC_CLAUDE_API_KEY=your_claude_api_key_here
OPENAI_API_KEY=your_openai_api_key_here

# Rate Limiting
NEXT_PUBLIC_RATE_LIMIT_REQUESTS=100
NEXT_PUBLIC_RATE_LIMIT_WINDOW=60000

# Webhook Configuration
WEBHOOK_SECRET=your_webhook_secret_here
WEBHOOK_URL=https://your-domain.com/api/webhooks

# Security
CORS_ORIGIN=http://localhost:3000
ALLOWED_ORIGINS=http://localhost:3000,https://labelmint.com